{
  "name": "youngjs",
  "version": "1.0.0",
  "description": "youngjs",
  "private": true,
  "main": "./app.ts",
  "scripts": {
    "start": "tsc && pm2 start pm2.json",
    "stop": "pm2 stop youngjs",
    "restart": "tsc && pm2 delete youngjs && pm2 start pm2.json",
    "reload": "tsc && pm2 reload youngjs",
    "build": "tsc",
    "dev": "nodemon app.ts"
  },
  "dependencies": {
    "@youngjs/core": "^1.0.31",
    "@youngjs/swagger-doc": "^1.0.6",
    "ali-oss": "^6.17.1",
    "ipip-ipdb": "^0.6.0",
    "jsonwebtoken": "^8.5.1",
    "svg-captcha": "^1.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.41",
    "ts-node": "^10.5.0",
    "typescript": "^4.7.2"
  }
}
